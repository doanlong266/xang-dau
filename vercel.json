{
    "version": 2,
    "builds": [
      {
        "src": "api/proxy.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/proxy.js"
      }
    ]
  }
  